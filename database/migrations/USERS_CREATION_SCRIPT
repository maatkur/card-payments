	[userCode] [varchar](5) NOT NULL,
	[userName] [varchar](50) NOT NULL,
	[password] [varchar](60) NOT NULL,
	[adminUser] [bit] NULL,
	[store] [varchar](3) NULL
) ON [PRIMARY]
